<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="codigohernancho.app.prueba.com.inventariodecompras.ENTRADAS"
    android:orientation="vertical"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp"
    android:baselineAligned="false"
    android:weightSum="1">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="155dp"
        android:layout_height="137dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_conversion_absoluteHeight="341dp"
        tools:layout_conversion_absoluteWidth="156dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <EditText
        android:id="@+id/txtstockMax"
        android:layout_width="173dp"
        android:layout_height="46dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Stock actual"
        tools:layout_conversion_absoluteHeight="42dp"
        tools:layout_conversion_absoluteWidth="384dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="341dp"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="0dp"/>

    <EditText
        android:id="@+id/txtcantidad"
        android:layout_width="173dp"
        android:layout_height="48dp"
        android:ems="10"
        android:hint="Cantidad"
        android:inputType="textPersonName"
        tools:layout_conversion_absoluteHeight="42dp"
        tools:layout_conversion_absoluteWidth="384dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="383dp"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="46dp" />

    <EditText
        android:id="@+id/txtcodigo"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:ems="10"
        android:hint="Codigo"
        android:inputType="textPersonName"
        tools:layout_conversion_absoluteHeight="42dp"
        tools:layout_conversion_absoluteWidth="384dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="426dp"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="94dp"
        android:layout_weight="0.02" />

    <EditText
        android:id="@+id/txtnombre"
        android:layout_width="384dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="textPersonName"
        tools:layout_conversion_absoluteHeight="42dp"
        tools:layout_conversion_absoluteWidth="384dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="468dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="153dp" />

    <EditText
        android:id="@+id/txtmarca"
        android:layout_width="379dp"
        android:layout_height="39dp"
        android:contentDescription="Marca"
        android:ems="10"
        android:hint="Marca"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="203dp" />

    <EditText
        android:id="@+id/txtunidad"
        android:layout_width="379dp"
        android:layout_height="42dp"
        android:ems="10"
        android:hint="Unidad"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="249dp" />

    <EditText
        android:id="@+id/txtdescripcion"
        android:layout_width="366dp"
        android:layout_height="53dp"
        android:ems="10"
        android:hint="Descripcion"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="298dp" />

    <EditText
        android:id="@+id/txtstockMin"
        android:layout_width="363dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Stock Minimo"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="390dp" />


</LinearLayout>
